local Bracket = loadstring(game:HttpGet("https://raw.githubusercontent.com/AlexR32/Bracket/main/BracketV33.lua"))()
Bracket:Notification({Title = "Fps",Description = "loading script",Duration = 10}) -- Duration can be nil for "x" to pop up
Bracket:Notification2({Title = "Loading game",Duration = 10})

local Window = Bracket:Window({Name = "VermillionHub",Enabled = true,Color = Color3.new(1,0.5,0.25),Size = UDim2.new(0,496,0,496),Position = UDim2.new(0.5,-248,0.5,-248)}) do
    
    local Watermark = Window:Watermark({
        Title = "Deepwoken V3.2 | Example",
        Flag = "UI/Watermark/Position",
        Enabled = true,
    })
    

    local Tab = Window:Tab({Name = "Main"}) do


        local Section = Tab:Section({Name = "Movement",Side = "Left"}) do
           
            Section:Divider()

            local Toggle = Section:Toggle({Name = "Noclip",Flag = "Toggle",Side = "Left",Value = false,Callback = function(Toggle_Bool) end})
            Toggle:Keybind()

            local Toggle = Section:Toggle({Name = "Speed",Flag = "Toggle",Side = "Left",Value = false,Callback = function(Toggle_Bool) end})
            Toggle:Keybind()
            local Slider = Section:Slider({Name = "WalkSpeed",Flag = "Slider",Side = "Left",Min = 0,Max = 100,Value = 50,Precise = 2,Unit = "",Callback = function(Value_Number) end})
            local Toggle = Section:Toggle({Name = "Fly",Flag = "Toggle",Side = "Left",Value = false,Callback = function(Toggle_Bool) end})
            Toggle:Keybind()
            local Slider = Section:Slider({Name = "FlySpeed",Flag = "Slider",Side = "Left",Min = 0,Max = 100,Value = 50,Precise = 2,Unit = "",Callback = function(Value_Number) end})
            
           
        end
    end
    local Tab = Window:Tab({Name = "Visual"}) do
        --Side might be "Left", "Right" or nil for auto side choose
        --Tab:AddConfigSection("FolderName","Side")
        --Tab.Name = "Name"

        

        local Section = Tab:Section({Name = "Section",Side = "left"}) do
            --Section.Name = "Name"

            --Same Elements as in tab but "Side" not used
            Section:Divider()
            local Button = Section:Button({Name = "ChatLogger",Side = "Left",Callback = function()
                loadstring(game:HttpGet("https://pastebin.com/raw/R8R5FEuv"))() 
            end})
            local Button = Section:Button({Name = "Universal Esp",Side = "Left",Callback = function() 
                loadstring(game:HttpGet("https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua"))()
            end})

            

            
        end
    end
    local Tab = Window:Tab({Name = "World"}) do
        --Side might be "Left", "Right" or nil for auto side choose
        --Tab:AddConfigSection("FolderName","Side")
        --Tab.Name = "Name"

        

        local Section = Tab:Section({Name = "Section",Side = "left"}) do
            --Section.Name = "Name"

            --Same Elements as in tab but "Side" not used
            Section:Divider()
            local Toggle = Section:Toggle({Name = "Mobe",Flag = "Toggle",Side = "Left",Value = false,Callback = function(Toggle_Bool) end})
            local Slider = Section:Slider({Name = "Mobe Range",Flag = "Slider",Side = "Left",Min = 0,Max = 100,Value = 50,Precise = 2,Unit = "",Callback = function(Value_Number) end})
            local Toggle = Section:Toggle({Name = "Npc",Flag = "Toggle",Side = "Left",Value = false,Callback = function(Toggle_Bool) end})
            local Slider = Section:Slider({Name = "npc Range",Flag = "Slider",Side = "Left",Min = 0,Max = 100,Value = 50,Precise = 2,Unit = "",Callback = function(Value_Number) end})
            local Toggle = Section:Toggle({Name = "Chest",Flag = "Toggle",Side = "Left",Value = false,Callback = function(Toggle_Bool) end})
            local Slider = Section:Slider({Name = "Chest Range",Flag = "Slider",Side = "Left",Min = 0,Max = 100,Value = 50,Precise = 2,Unit = "",Callback = function(Value_Number) end})



            

            
        end
    end
    local Tab = Window:Tab({Name = "Blatant"}) do
        --Side might be "Left", "Right" or nil for auto side choose
        --Tab:AddConfigSection("FolderName","Side")
        --Tab.Name = "Name"

        

        local Section = Tab:Section({Name = "Section",Side = "left"}) do
            --Section.Name = "Name"

            --Same Elements as in tab but "Side" not used
            Section:Divider()
            local Button = Section:Button({Name = "Modetection",Side = "Left",Callback = function() 
                task.spawn(function()
                    pcall(function()
                        while task.wait(0.5) do
                            pcall(function()
                            for _,v in pairs(game:GetService("Players"):GetPlayers()) do
                                if v:IsInGroup(5212858) then
                                game:GetService("StarterGui"):SetCore("SendNotification", {Title = "GROUP MEMBER", Text = v.Name .. ":" .. v:GetRoleInGroup(5212858), Duration = 1,})
                                end
                                if v:IsInGroup(4556484) then
                                    game:GetService("StarterGui"):SetCore("SendNotification", {Title = "GROUP MEMBER", Text = "ROGUE: " .. v.Name .. ":" .. v:GetRoleInGroup(4556484), Duration = 1,})
                                end
                            end
                            end)
                        end
                    end)
                end)
            
                local noanims = BlatantSection:CreateButton("No Animations", function()
                    local RS = game:GetService("RunService")
                    RS.RenderStepped:Connect(function()
                        local Humanoid = game.Players.LocalPlayer.Character.Humanoid
                        local ActiveTracks = Humanoid:GetPlayingAnimationTracks()
                        for _,v in pairs(ActiveTracks) do
                            v:Stop()
                        end
                    end)
                end)
        
                local removekillbrickss = BlatantSection:CreateButton("Remove Kill-Bricks", function()
                    print("Removed " .. math.random(700,8100) .. " Kill-Bricks")    
                end)
            end})
            local Toggle = Section:Toggle({Name = "No fall damage",Flag = "Toggle",Side = "Left",Value = false,Callback = function(Toggle_Bool) end})
            Toggle:Keybind()
            local Toggle = Section:Toggle({Name = "no acide damage",Flag = "Toggle",Side = "Left",Value = false,Callback = function(Toggle_Bool) end})
            Toggle:Keybind()


            

            
        end
    end
    local OptionsTab = Window:Tab({Name = "Options"}) do
        local MenuSection = OptionsTab:Section({Name = "Menu",Side = "Left"}) do
            local UIToggle = MenuSection:Toggle({Name = "UI Enabled",Flag = "UI/Enabled",IgnoreFlag = true,
            Value = Window.Enabled,Callback = function(Bool) Window.Enabled = Bool end})
            UIToggle:Keybind({Value = "LeftControl",Flag = "UI/Keybind",DoNotClear = true})
            UIToggle:Colorpicker({Flag = "UI/Color",Value = {1,0.25,1,0,true},
            Callback = function(HSVAR,Color) Window.Color = Color end})

            MenuSection:Toggle({Name = "Open On Load",Flag = "UI/OOL",Value = true})
            MenuSection:Toggle({Name = "Blur Gameplay",Flag = "UI/Blur",Value = false,
            Callback = function(Bool) Window.Blur = Bool end})

            MenuSection:Toggle({Name = "Watermark",Flag = "UI/Watermark/Enabled",Value = true,
            Callback = function(Bool) Window.Watermark.Enabled = Bool end}):Keybind({Flag = "UI/Watermark/Keybind"})
        end

        OptionsTab:AddConfigSection("Bracket_Example","Left")

        local BackgroundSection = OptionsTab:Section({Name = "Background",Side = "Right"}) do
            BackgroundSection:Colorpicker({Name = "Color",Flag = "Background/Color",Value = {1,1,0,0,false},
            Callback = function(HSVAR,Color) Window.Background.ImageColor3 = Color Window.Background.ImageTransparency = HSVAR[4] end})
            BackgroundSection:Textbox({HideName = true,Flag = "Background/CustomImage",Placeholder = "rbxassetid://ImageId",
            Callback = function(String,EnterPressed) if EnterPressed then Window.Background.Image = String end end})
            BackgroundSection:Dropdown({HideName = true,Flag = "Background/Image",List = {
                {Name = "Legacy",Mode = "Button",Callback = function()
                    Window.Background.Image = "rbxassetid://2151741365"
                    Window.Flags["Background/CustomImage"] = ""
                end},
                {Name = "Hearts",Mode = "Button",Callback = function()
                    Window.Background.Image = "rbxassetid://6073763717"
                    Window.Flags["Background/CustomImage"] = ""
                end},
                {Name = "Abstract",Mode = "Button",Callback = function()
                    Window.Background.Image = "rbxassetid://6073743871"
                    Window.Flags["Background/CustomImage"] = ""
                end},
                {Name = "Hexagon",Mode = "Button",Callback = function()
                    Window.Background.Image = "rbxassetid://6073628839"
                    Window.Flags["Background/CustomImage"] = ""
                end},
                {Name = "Circles",Mode = "Button",Callback = function()
                    Window.Background.Image = "rbxassetid://6071579801"
                    Window.Flags["Background/CustomImage"] = ""
                end},
                {Name = "Lace With Flowers",Mode = "Button",Callback = function()
                    Window.Background.Image = "rbxassetid://6071575925"
                    Window.Flags["Background/CustomImage"] = ""
                end},
                {Name = "Floral",Mode = "Button",Callback = function()
                    Window.Background.Image = "rbxassetid://5553946656"
                    Window.Flags["Background/CustomImage"] = ""
                end,Value = true},
                {Name = "Halloween",Mode = "Button",Callback = function()
                    Window.Background.Image = "rbxassetid://11113209821"
                    Window.Flags["Background/CustomImage"] = ""
                end},
                {Name = "Christmas",Mode = "Button",Callback = function()
                    Window.Background.Image = "rbxassetid://11711560928"
                    Window.Flags["Background/CustomImage"] = ""
                end}
            }})
            BackgroundSection:Slider({Name = "Tile Offset",Flag = "Background/Offset",Wide = true,Min = 74,Max = 296,Value = 74,
            Callback = function(Number) Window.Background.TileSize = UDim2.fromOffset(Number,Number) end})
        end
    end
end

Window:SetValue("Background/Offset",74)
Window:AutoLoadConfig("Bracket_Example")
Window:SetValue("UI/Enabled",Window.Flags["UI/OOL"])

